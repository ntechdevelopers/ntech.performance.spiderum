import net.minidev.json.parser.JSONParser;
import org.apache.commons.lang.StringUtils;
import java.util.List;
import java.util.ArrayList;

// Parser data
String jsonString = new String(data);
JSONParser parser = new JSONParser(JSONParser.MODE_JSON_SIMPLE);

JSONObject store = (JSONObject) parser.parse(data);

JSONArray followings = (JSONArray) store.get(&quot;followings&quot;);
int totalItemsInPage = (int) followings.size();

log.info(&quot;Log totalItemsInPage: &quot; + totalItemsInPage);
log.info(&quot;Log followings: &quot; + followings);

String fileName = vars.get(&quot;csv_file_name&quot;);

BufferedReader reader;
FileReader fstreamRead;
File inputFile = new File(fileName);

ArrayList oldData = new ArrayList();
int isEmptyFile = (int) inputFile.length();
if(inputFile.exists() &amp;&amp; !inputFile.isDirectory() &amp;&amp; isEmptyFile != 0) { 
    FileReader fstreamRead = new FileReader(inputFile);
    reader = new BufferedReader(fstreamRead);

    while((currentLine = reader.readLine()) != null) {
      	// Trim newline when comparing
      	String trimmedLine = (String) currentLine.trim();
      	oldData.add(trimmedLine);
	}

	reader.close();
	fstreamRead.close();
}

FileWriter fstreamWrite = new FileWriter(fileName, true);
BufferedWriter writer = new BufferedWriter(fstreamWrite);

// Export csv
for (Object item : followings) {
     JSONObject item = (JSONObject) item;
	JSONObject follower = (JSONObject) item.get(&quot;B_id&quot;);
	
     int userId = (int) follower.get(&quot;_id&quot;);
     String name = (String) follower.get(&quot;name&quot;);
     String newData = userId + &quot;,&quot; + name;

	// Check duplicate data
	boolean isDuplicate = oldData.contains(newData);
	if(!isDuplicate){
		log.info(&quot;Log user: &quot; + newData);
		writer.write( userId + &quot;,&quot; + name);
 		writer.write(System.getProperty(&quot;line.separator&quot;));
	}
	else{
		log.info(&quot;Log duplidate data. &quot; + newData);
	}
}

writer.close();
fstreamWrite.close();


