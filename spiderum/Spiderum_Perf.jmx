            <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="Extract data user" enabled="true">
              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="script">import net.minidev.json.JSONArray;
import net.minidev.json.JSONObject;
import net.minidev.json.parser.JSONParser;
import org.apache.commons.lang.StringUtils;
import java.util.List;
import java.util.ArrayList;

// Parser data
String jsonString = new String(data);
JSONParser parser = new JSONParser(JSONParser.MODE_JSON_SIMPLE);

JSONObject store = (JSONObject) parser.parse(data);

JSONArray createdPosts = (JSONArray) store.get(&quot;createdPosts&quot;);
int totalCreatedPostsInPage = (int) createdPosts.size();

log.info(&quot;Log totalCreatedPostsInPage: &quot; + totalCreatedPostsInPage);
log.info(&quot;Log createdPosts: &quot; + createdPosts);
vars.putObject(&quot;totalCreatedPostsInPage&quot;, totalCreatedPostsInPage);

String folder = vars.get(&quot;posts_folder_output&quot;);
String name = vars.get(&quot;name_get_posts&quot;);
String fileName = folder + &quot;Post_&quot; + name + &quot;.csv&quot;;
log.info(&quot;Log fileName: &quot; + fileName);

BufferedReader reader;
FileReader fstreamRead;
File inputFile = new File(fileName);

ArrayList oldData = new ArrayList();
int isEmptyFile = (int) inputFile.length();
if(inputFile.exists() &amp;&amp; !inputFile.isDirectory() &amp;&amp; isEmptyFile != 0) { 
    FileReader fstreamRead = new FileReader(inputFile);
    reader = new BufferedReader(fstreamRead);

    while((currentLine = reader.readLine()) != null) {
      	// Trim newline when comparing
      	String trimmedLine = (String) currentLine.trim();
      	oldData.add(trimmedLine);
	}

	reader.close();
	fstreamRead.close();
}

FileWriter fstreamWrite = new FileWriter(fileName, true);
BufferedWriter writer = new BufferedWriter(fstreamWrite);

// Export csv
for (Object item : createdPosts) {
     JSONObject item = (JSONObject) item;
	
     int postId = (int) item.get(&quot;_id&quot;);
     String postSlug = (String) item.get(&quot;slug&quot;);
     point = item.get(&quot;point&quot;);
     commentCount = item.get(&quot;comment_count&quot;);
     star = item.get(&quot;star&quot;);
     createdAt = item.get(&quot;created_at&quot;);
     newData = postId + &quot;,&quot; + postSlug + &quot;,&quot; + point + &quot;,&quot; + commentCount + &quot;,&quot; + star + &quot;,&quot; + createdAt;

	// Check duplicate data
	boolean isDuplicate = oldData.contains(newData);
	if(!isDuplicate){
		log.info(&quot;Log post: &quot; + newData);
		writer.write(newData);
 		writer.write(System.getProperty(&quot;line.separator&quot;));
	}
	else{
		log.info(&quot;Log duplidate data. &quot; + newData);
	}
}

writer.close();
fstreamWrite.close();


</stringProp>
