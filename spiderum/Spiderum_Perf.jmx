// Parser data
String jsonString = new String(data);
JSONParser parser = new JSONParser(JSONParser.MODE_JSON_SIMPLE);
JSONObject store = (JSONObject) parser.parse(data);

JSONObject profile = (JSONObject) store.get(&quot;profile&quot;);
JSONObject count = (JSONObject) store.get(&quot;count&quot;);
JSONObject getExternalInfo = (JSONObject) count.get(&quot;getExternalInfo&quot;);

int userId = (int) profile.get(&quot;_id&quot;);
String name = (String) profile.get(&quot;name&quot;);
displayName = profile.get(&quot;display_name&quot;);
email = profile.get(&quot;email&quot;);
score = profile.get(&quot;score&quot;);

birthDay = getExternalInfo.get(&quot;birth_day&quot;);
gender = getExternalInfo.get(&quot;gender&quot;);

followers = count.get(&quot;followers&quot;);
followings = count.get(&quot;followings&quot;);
comments = count.get(&quot;comments&quot;);
createdPosts = count.get(&quot;createdPosts&quot;);

newData = userId + &quot;,&quot; + 
  		name + &quot;,&quot; + 
	  	displayName + &quot;,&quot; + 
	  	email + &quot;,&quot; + 
	  	gender + &quot;,&quot; + 
	  	birthDay + &quot;,&quot; + 
	  	score + &quot;,&quot; + 
	  	followers + &quot;,&quot; + 
	  	followings + &quot;,&quot; + 
	  	comments + &quot;,&quot; + 
	  	createdPosts;

String fileName = vars.get(&quot;csv_file_user_detail&quot;);

BufferedReader reader;
FileReader fstreamRead;
File inputFile = new File(fileName);

ArrayList oldData = new ArrayList();
int isEmptyFile = (int) inputFile.length();
if(inputFile.exists() &amp;&amp; !inputFile.isDirectory() &amp;&amp; isEmptyFile != 0) { 
    FileReader fstreamRead = new FileReader(inputFile);
    reader = new BufferedReader(fstreamRead);

    while((currentLine = reader.readLine()) != null) {
      	// Trim newline when comparing
      	String trimmedLine = (String) currentLine.trim();
      	oldData.add(trimmedLine);
	}

	reader.close();
	fstreamRead.close();
}

FileWriter fstreamWrite = new FileWriter(fileName, true);
//OutputStreamWriter fstreamWrite = new OutputStreamWriter(new FileOutputStream(inputFile, true), &quot;UTF-8&quot;);

BufferedWriter writer = new BufferedWriter(fstreamWrite);

// Export csv
// Check duplicate data
boolean isDuplicate = oldData.contains(newData);
if(!isDuplicate){
	log.info(&quot;Log newdata: &quot; + newData);
	writer.write(newData);
	writer.write(System.getProperty(&quot;line.separator&quot;));
	
	//writer.append(newData);
	//writer.newLine();
}
else{
	log.info(&quot;Log duplidate data. &quot; + newData);
}

writer.close();
fstreamWrite.close();


